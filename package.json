{
  "name": "nuxt-full-stack",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "cross-env NODE_ENV=development nuxt dev",
    "dev:prod": "cross-env NODE_ENV=production nuxt dev",
    "build": "cross-env NODE_ENV=production nuxt build",
    "build:analyze": "cross-env NODE_ENV=production ANALYZE=true nuxt build",
    "generate": "cross-env NODE_ENV=production nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "start": "cross-env NODE_ENV=production node .output/server/index.mjs",
    "start:dev": "cross-env NODE_ENV=development node .output/server/index.mjs",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:push:dev": "cross-env NODE_ENV=development prisma db push",
    "db:push:prod": "cross-env NODE_ENV=production prisma db push",
    "db:studio": "prisma studio",
    "db:studio:dev": "cross-env NODE_ENV=development prisma studio",
    "db:studio:prod": "cross-env NODE_ENV=production prisma studio",
    "db:seed": "cross-env NODE_ENV=development tsx prisma/seed.ts",
    "db:reset": "cross-env NODE_ENV=development prisma migrate reset",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "type-check": "vue-tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage"
  },
  "dependencies": {
    "@lucia-auth/adapter-prisma": "^4.0.1",
    "@nuxt/ui": "^2.12.2",
    "@prisma/client": "^5.22.0",
    "@trpc/client": "^10.45.0",
    "@trpc/server": "^10.45.0",
    "bcrypt": "^5.1.1",
    "lucia": "^3.0.1",
    "nuxt": "^3.17.5",
    "prisma": "^5.22.0",
    "superjson": "^2.2.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@nuxt/eslint-config": "^0.2.0",
    "@types/bcrypt": "^5.0.2",
    "@vitest/ui": "^1.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.56.0",
    "prettier": "^3.1.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vitest": "^1.0.0",
    "vue-tsc": "^2.2.10"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "packageManager": "pnpm@8.12.1"
}